<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="0dp"
        android:height="50dp"
        android:bottom="8dp">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/white">
            </solid>
            <stroke
                android:width="1dp"
                android:color="@color/black">
            </stroke>
            <corners
                android:radius="25dp">
            </corners>
        </shape>
    </item>
</layer-list>